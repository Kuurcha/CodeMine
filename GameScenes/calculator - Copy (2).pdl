Program         = Command;

Command         = MoveCommand;

MoveCommand     = "Move" Id Direction Steps ;

Id              = /[a-zA-Z_][a-zA-Z0-9_]*/ ;

Direction       = "left" | "right" | "up" | "down" ;

Steps           = Number ;

QuotedText      = '"' (!'"' .)* '"' ;

Number          = /[0-9]+/ ;

Whitespace      ~ /[\s]+/ ;

:start          = Program ;
:ignore         = Whitespace ;